---
layout: "_layouts/default.latte"
title: "Czech Posts"
id: blog
---

{block content}
    <div class="container-fluid" id="blog">
        <h1>Czech Posts</h1>

        <div class="row">
            {foreach $posts as $post}
                {continueIf ! isset($post['deprecated']) || ! isset($post['lang'])}

                {include "_snippets/post/singlePostPreview.latte", post => $post}
            {/foreach}
        </div>
    </div>
{/block}
